{
  "name": "s5-utils-js-dev",
  "version": "0.1.0-beta8-3-3-5",
  "description": "S5 Utils Javascript",
  "type": "module",
  "types": "dist/mjs/index.d.ts",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "browserslist": [
    "defaults",
    "not IE 11",
    "not OperaMini all"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json",
    "lint": "yarn lint:tsc && yarn lint:eslint",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:tsc": "tsc",
    "prettier": "prettier --write src/*.ts && prettier --write src/utils/*.ts",
    "prepublishOnly": "yarn && yarn build",
    "test": "echo test",
    "webpack": "npx webpack --config ./webpack.config.cjs",
    "webpack-index": "npx webpack --config ./webpack.index.config.cjs"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,yml,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parajbs-dev/s5-utils-js.git"
  },
  "keywords": [
    "Decentralised",
    "Content-Addressed",
    "Cloud Storage",
    "S5-Utils",
    "S5"
  ],
  "author": "parajbs-dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parajbs-dev/s5-utils-js/issues"
  },
  "homepage": "https://github.com/parajbs-dev/s5-utils-js",
  "dependencies": {
    "@noble/hashes": "^1.3.1",
    "buffer": "^6.0.3",
    "mime": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/mime": "^3.0.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-loader": "^9.1.2",
    "eslint": "^8.43.0",
    "eslint-plugin-jsdoc": "46.2.6",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "typescript": "5.0.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  }
}
